<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net5.0</TargetFrameworks>
    <Id>ConfigAdapter.HJson</Id>
    <Title>ConfigAdapter.HJson</Title>
    <Version>3.2.0</Version>
    <Authors>carlubian</Authors>
    <Owners>carlubian</Owners>
    <Company>carlubian</Company>
    <PackageReleaseNotes>Update to NET 5.0.</PackageReleaseNotes>
    <PackageIconUrl>http://carlubian.azurewebsites.net/images/icon-ConfigAdapterV2.png</PackageIconUrl>
    <Description>Provides an adapter to use .HJSON files in ConfigAdapter.</Description>
    <RepositoryUrl>https://github.com/carlubian/ConfigAdapter</RepositoryUrl>
    <PackageProjectUrl>https://github.com/carlubian/ConfigAdapter</PackageProjectUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <PackageTags>configuration-files, dotnet-standard, dotnet</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ConfigAdapter" Version="2.3.0" />
    <PackageReference Include="Hjson" Version="3.0.0" />
  </ItemGroup>

</Project>
